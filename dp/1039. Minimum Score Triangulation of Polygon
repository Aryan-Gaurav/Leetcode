problem link https://leetcode.com/problems/minimum-score-triangulation-of-polygon/

code
class Solution {
public:
int minScoreTriangulation(vector<int>& v) 
{
    int n=v.size();
    int dp[n][n];
    memset(dp,0,sizeof(dp));
    for(int len=3;len<=n;len++)
    {
        for(int i=0;i<n;i++)
        {
            int j=i+len-1;
            dp[i][j%n]=1e9;
            for(int k=i+1;k<j;k++)
                dp[i][j%n]=min(dp[k%n][j%n]+dp[i][k%n]+v[i]*v[j%n]*v[k%n],dp[i][j%n]);
        }
    }
    return dp[0][n-1];
}
};
